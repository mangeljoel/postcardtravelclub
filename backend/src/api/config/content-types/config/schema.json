{
  "kind": "singleType",
  "collectionName": "configs",
  "info": {
    "singularName": "config",
    "pluralName": "configs",
    "displayName": "config",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "version": {
      "type": "integer"
    },
    "mediaPath": {
      "type": "string"
    },
    "headerTitle": {
      "type": "string"
    },
    "headerSubTitle": {
      "type": "text"
    },
    "showFeaturedAlbums": {
      "type": "boolean",
      "default": true
    },
    "showFeaturedPostcards": {
      "type": "boolean",
      "default": true
    },
    "maxFeaturedAlbums": {
      "type": "integer",
      "default": 10
