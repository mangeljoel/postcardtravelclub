    },
    "sharedSeo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "publications": {
      "type": "text"
    },
    "ourStoryDescription": {
      "type": "text"
    },
    "supportLink": {
      "type": "string"
    },
    "featuredAlbums": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::album.album"
    },
    "featuredPostcards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::postcard.postcard"
    }
