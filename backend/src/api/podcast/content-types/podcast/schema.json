{
  "kind": "collectionType",
  "collectionName": "podcasts",
  "info": {
    "singularName": "podcast",
    "pluralName": "podcasts",
    "displayName": "Podcast",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "text"
    },
    "album": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::album.album"
    },
    "pubDate": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "preview": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "spotifyUrl": {
      "type": "text"
    },
    "appleUrl": {
      "type": "text"
    },
    "googleUrl": {
      "type": "text"
    },
    "podcastUrl": {
      "type": "text"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
